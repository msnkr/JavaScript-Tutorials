<body>
  <div class="container-2">
    <h4>
      Select the activities you would like to try and we'll match them with the
      locations we offer
    </h4>
    <% activities.forEach(activity => { %>
    <div class="activity-list">
      <label for="activity <%= activity %>"><%= activity %></label>
      <input type="checkbox" name="activity" id="activity" />
    </div>
    <% }) %>
  </div>
</body>
